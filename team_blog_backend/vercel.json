{
  "version": 2,
  "builds": [
    {
      "src": "blog_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "blog_project/wsgi.py"
    },
    {
      "src": "/media/(.*)",
      "dest": "blog_project/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "blog_project/wsgi.py"
    }
  ]
}